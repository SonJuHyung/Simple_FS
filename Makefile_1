.SUFFIX : .c .o

OBJECTS = bitmap.o dir.o inode.o disk_map.o fsdriver.o fsformat.o file.o
SRCS = bitmap.c dir.c inode.c disk_map.c fsdriver.c fsformat.c file.c

CC = gcc
INC = -l /home/son/dku/Lab/Lab4_fuse/Lab4/include
CFLAGS = -g -c

TARGET = Lab4

$(TARGET) : $(OBJECTS)
	$(CC) -o $(TARGET) $(OBJECTS)

.c.o :
	$(CC) $(INC) $(CFLAGS) $<

clean :
	rm -rf $(OBJECTS) $(TARGET) core

dep : 
	gccmaedep $(SRCS)


